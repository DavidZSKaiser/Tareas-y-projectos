Algoritmo TablaMultiplicar
	//definir variables
	Definir numero_1,tabla_1, resultado Como Entero;
	//ingresar numero
	Escribir "ingresar numero  ";
	Leer numero_1;
	//aqui se realiza la operacion
	Para tabla_1<-1 Hasta 10 Hacer
		Escribir numero_1, " x ",tabla_1," = ", numero_1*tabla_1;
		resultado = numero_1 + tabla_1;
	FinPara
	

FinAlgoritmo
