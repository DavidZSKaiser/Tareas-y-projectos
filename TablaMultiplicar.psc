Algoritmo TablaMultiplicar
	//definir variables
	Definir numero_1,tabla, resultado Como Entero;
	//ingresar numero
	Escribir "ingresar numero  ";
	Leer numero_1;
	//aqui se realiza la operacion
	Para tabla<-1 Hasta 10 Hacer
		Escribir numero_1, " x ",tabla," = ", numero_1*tabla;
		resultado = numero_1 + tabla;
	FinPara
	

FinAlgoritmo
